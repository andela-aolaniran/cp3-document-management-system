{
  "name": "cp3-document-management-system",
  "version": "1.0.0",
  "description": "A simple document management system api, with a web app which consumes the api",
  "main": "index.js",
  "scripts": {
    "mocha": "NODE_ENV=test mocha --compilers js:babel-core/register server/tests client/tests",
    "migrate": "sequelize db:migrate",
    "seed": "NODE_ENV=test babel-node server/tests/helpers/SeedHelper.js",
    "test": "npm run seed && npm run mocha",
    "lint": "node_modules/.bin/esw webpack.config.* server client",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run test -- --watch",
    "start": "babel-node app.js",
    "develop:server": "npm-run-all --parallel lint:watch test:watch open:server",
    "open:server": "NODE_ENV=development nodemon app.js --exec babel-node",
    "develop_server": "./node_modules/gulp/bin/gulp.js develop_server",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec server/tests client/tests",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-aolaniran/cp3-document-management-system.git"
  },
  "keywords": [
    "document",
    "management",
    "system",
    "api"
  ],
  "author": "Azeez Olaniran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-aolaniran/cp3-document-management-system/issues"
  },
  "homepage": "https://github.com/andela-aolaniran/cp3-document-management-system#readme",
  "devDependencies": {
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "coveralls": "^2.11.15",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "istanbul": "^0.4.5",
    "jsdom": "8.5.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "run-sequence": "^1.2.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "6.8.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "faker": "^3.1.0",
    "fm-log": "^4.4.3",
    "jquery": "2.2.3",
    "jsonwebtoken": "^7.2.1",
    "materialize-css": "^0.98.0",
    "morgan": "^1.7.0",
    "open": "0.0.5",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "sequelize": "^3.30.0",
    "sequelize-cli": "^2.4.0",
    "superagent": "^3.4.4",
    "supertest": "^3.0.0",
    "toastr": "2.1.2"
  }
}
